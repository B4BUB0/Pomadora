{
  "name": "FocusSession",
  "type": "object",
  "properties": {
    "duration_minutes": {
      "type": "number",
      "description": "Duration of the focus session in minutes"
    },
    "completed_at": {
      "type": "string",
      "format": "date-time",
      "description": "When the session was completed"
    },
    "tasks_completed": {
      "type": "number",
      "description": "Number of tasks completed during session"
    },
    "visualization_type": {
      "type": "string",
      "enum": ["water", "sand"],
      "description": "Type of visualization used"
    }
  },
  "required": ["duration_minutes", "completed_at"]
}
